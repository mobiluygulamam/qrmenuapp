--
-- Table structure for table `qr_orders`
--

DROP TABLE IF EXISTS `qr_orders`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `qr_orders` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `restaurant_id` int(11) DEFAULT NULL,
  `type` enum('on-table','takeaway','delivery') NOT NULL DEFAULT 'on-table',
  `customer_name` varchar(255) DEFAULT NULL,
  `table_number` int(11) DEFAULT NULL,
  `phone_number` varchar(25) DEFAULT NULL,
  `address` varchar(255) DEFAULT NULL,
  `status` enum('pending','completed','unpaid') NOT NULL DEFAULT 'pending',
  `message` varchar(255) DEFAULT NULL,
  `seen` tinyint(1) NOT NULL DEFAULT 0,
  `is_paid` tinyint(1) NOT NULL DEFAULT 0,
  `payment_gateway` varchar(25) DEFAULT NULL,
  `created_at` datetime DEFAULT NULL,
  `device_user_id` bigint(20) unsigned DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `restaurant_id` (`restaurant_id`),
  KEY `qr_orders_table_number_foreign` (`table_number`),
  KEY `qr_orders_device_user_id_foreign` (`device_user_id`),
  CONSTRAINT `qr_orders_device_user_id_foreign` FOREIGN KEY (`device_user_id`) REFERENCES `qr_device_users` (`id`) ON DELETE CASCADE,
  CONSTRAINT `qr_orders_table_number_foreign` FOREIGN KEY (`table_number`) REFERENCES `qr_tables` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

