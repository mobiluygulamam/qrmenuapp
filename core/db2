/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `qr_upgrades` (
  `upgrade_id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `sub_id` varchar(16) NOT NULL DEFAULT '0',
  `user_id` int(11) NOT NULL DEFAULT 0,
  `pay_mode` enum('one_time','recurring') NOT NULL DEFAULT 'one_time',
  `interval` enum('MONTHLY','YEARLY','LIFETIME') DEFAULT NULL,
