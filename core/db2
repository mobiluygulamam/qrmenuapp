CREATE TABLE `qr_tables` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userid` int(11) NOT NULL,
  `restoid` int(11) NOT NULL,
  `table_id` bigint(20) NOT NULL,
  `url` text NOT NULL,
  `created_at` varchar(255) NOT NULL DEFAULT 'CURRENT_TIMESTAMP',
  `updated_at` varchar(255) DEFAULT NULL,
  `qr_text` varchar(255) NOT NULL,
  `status` varchar(255) NOT NULL DEFAULT 'available',
  PRIMARY KEY (`id`),
  KEY `qr_tables_userid_foreign` (`userid`),
  KEY `qr_tables_restoid_foreign` (`restoid`),
  CONSTRAINT `qr_tables_restoid_foreign` FOREIGN KEY (`restoid`) REFERENCES `qr_posts` (`id`),
  CONSTRAINT `qr_tables_userid_foreign` FOREIGN KEY (`userid`) REFERENCES `qr_user` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=81 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
